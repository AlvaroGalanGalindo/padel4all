{% extends 'base.html.twig' %}

{% block body %}
    <h1>Resultado list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Set1p1</th>
                <th>Set1p2</th>
                <th>Set2p1</th>
                <th>Set2p2</th>
                <th>Set3p1</th>
                <th>Set3p2</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for resultado in resultados %}
            <tr>
                <td><a href="{{ path('resultados_show', { 'id': resultado.id }) }}">{{ resultado.id }}</a></td>
                <td>{{ resultado.set1p1 }}</td>
                <td>{{ resultado.set1p2 }}</td>
                <td>{{ resultado.set2p1 }}</td>
                <td>{{ resultado.set2p2 }}</td>
                <td>{{ resultado.set3p1 }}</td>
                <td>{{ resultado.set3p2 }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('resultados_show', { 'id': resultado.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('resultados_edit', { 'id': resultado.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resultados_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
